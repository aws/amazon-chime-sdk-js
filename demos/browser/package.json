    "html-webpack-plugin": "^5.3.2",
    "open-iconic": "^1.1.1",
    "postcss-loader": "^6.1.1",
    "react-dev-utils": "^12.0.1",
    "sass": "^1.26.11",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.3.0",
    "typescript": "4.2.3",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.9.0"
  },
